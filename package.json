{
  "name": "js-packaging-demo",
  "version": "1.0.0",
  "author": "Igor Minar",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:prod": "rm -rf dist && webpack --config webpack.config.js",
    "build:debug": "rm -rf dist && webpack --config webpack.debug.config.js",
    "watch:prod": "yarn build:prod && watch -p 'src/*' -p 'webpack.*.js' -c 'yarn build:prod'",
    "watch:debug": "yarn build:debug && watch -p 'src/*' -p 'webpack.*.js' -c 'yarn build:debug'",
    "serve": "serve dist/"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "serve": "^6.3.1",
    "watch-cli": "^0.2.3",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "html-webpack-plugin": "^2.30.1",
    "rxjs": "^5.5.0"
  }
}
